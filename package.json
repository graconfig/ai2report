{
  "type": "module",
  "name": "ai2report",
  "version": "0.0.1",
  "description": "AI2REPORT CAP project.",
  "repository": "https://rdc.hand-china.com/gitlab/3749/ai2report.git",
  "license": "UNLICENSED",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map"
  ],
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap-ai-sdk/ai-api": "workspace:^",
    "@sap-ai-sdk/foundation-models": "workspace:^",
    "@sap-ai-sdk/orchestration": "workspace:^",
    "@sap-ai-sdk/langchain": "workspace:^",
    "@sap/cds": "^8.4.2",
    "@sap/xssec": "^4",
    "ai2report": "file:",
    "express": "^4.21.1",
    "langchain": "0.3.6",
    "@langchain/core": "0.3.18",
    "@langchain/textsplitters": "0.1.0",
    "@sap-cloud-sdk/util": "^3.23.0",
    "@types/express": "^5.0.0"
  },
  "devDependencies": {
    "@cap-js/cds-typer": ">=0.1",
    "@cap-js/cds-types": "^0.7.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8.4.2",
    "@types/node": "^22.0.0",
    "typescript": "^5"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "pnpm cds-build && pnpm compile && pnpm cleanup",
    "cds-build": "cds build --production",
    "compile": "tsc",
    "cleanup": "rm -f ./dist/srv/srv/**/*.ts ./dist/srv/package-lock.json",
    "postbuild": "pushd ./dist/srv && npm i --package-lock-only && popd",
    "start": "npx cds-serve",
    "watch": "cds-tsx watch",
    "watch:hybrid": "cds-tsx watch --profile hybrid",
    "lint": "eslint . && prettier . --config ../.prettierrc --ignore-path ../.prettierignore -c",
    "lint:fix": "eslint . --fix && prettier . --config ../.prettierrc --ignore-path ../.prettierignore -w --log-level error"
  },
  "cds": {
    "requires": {
      "[production]": {
        "auth": "xsuaa",
        "db": {
          "kind": "hana"
        }
      },
      "db": {
        "kind": "sqlite",
        "credentials": {
          "url": "db/chat.sqlite"
        }
      },
      "zui_zye9001_001": {
        "kind": "odata",
        "model": "srv/external/zui_zye9001_001",
        "csrf": true,
        "[production]": {
          "credentials": {
            "path": "/sap/opu/odata4/sap/zui_zye9001_001/srvd/sap/zui_zye9001_001/0001",
            "destination": "S4A_280_HTTPS"
          }
        }
      }
    },
    "sql": {
      "native_hana_associations": false
    }
  },
  "imports": {
    "#cds-models/*": "./@cds-models/*/index.js"
  }
}
